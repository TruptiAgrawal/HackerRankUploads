#include <stdio.h>
#include <string.h>
#include <math.h>
#include <stdlib.h>
#include <stdio.h>

int sumOfDigits(int n) {
    int sum = 0;
    while (n > 0) {
        sum += n % 10; 
        n /= 10; 
    }
    return sum;
}

int main() 
{
    int n;
    scanf("%d", &n); 
    printf("%d\n", sumOfDigits(n));
    return 0;
}
