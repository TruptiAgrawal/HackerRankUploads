#include <stdio.h>
#include <string.h>
#include <math.h>
#include <stdlib.h>

int main() 
{
    char ch ;
    char s[100];
    char sen[100];
    
    scanf("%c",&ch); // can only write a char
    scanf("%s",s); // can only write a word
    scanf(" %[^\n]",sen); // used for sentence writing
    
    printf("%c\n",ch);
    printf("%s\n",s);
    printf("%s\n",sen);

    /* Enter your code here. Read input from STDIN. Print output to STDOUT */    
    return 0;
}
