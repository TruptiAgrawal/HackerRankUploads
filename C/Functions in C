#include <stdio.h>
/*
Add `int max_of_four(int a, int b, int c, int d)` here.
*/

int max_of_four(int a, int b, int c, int d){

    int max = 0 ;

    {if (max>a){max=max;}
    else max=a;}
    {if (max>b){max=max;}
    else max=b;}
    {if (max>c){max=max;}
    else max=c;}
    {if (max>d){max=max;}
    else max=d;}
    
    return max;
}

int main() {
    int a, b, c, d;
    scanf("%d %d %d %d", &a, &b, &c, &d);
    int ans = max_of_four(a, b, c, d);
    printf("%d", ans);
    
    return 0;
}
